{
  "manifest_version": 2,
  "name": "dotjpg",
  "key":"",
  "description": "Send selected text from the browser to a google doc.",
  "version": "0.01",
  "author": "Gus Leyva",
  "background": {
  "scripts": ["js/background.js"]
  },
  "icons": {
    "16": "dotjpg.png",
    "32": "dotjpg.png",
    "48": "dotjpg.png",
    "128": "dotjpg.png"},
  "permissions":["identity","contextMenus","storage","notifications","tabs","activeTab","clipboardWrite"],  
  "browser_action": {
   "default_icon": "dotjpg.png",
   "default_popup": "dotjpg.html"
  }     
}