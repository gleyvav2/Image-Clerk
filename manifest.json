{
  "manifest_version": 2,
  "name": "Image Clerk",
  "key":"",
  "description": "Search google's images, copy, download and paste fast",
  "version": "0.01",
  "author": "Gus Leyva",
  "background": {
  "scripts": ["js/background.js","js/glider.js"]
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"},
  "permissions":["identity","contextMenus","storage","notifications","tabs","activeTab","clipboardWrite"],  
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "imageclerk.html"
  }     
}