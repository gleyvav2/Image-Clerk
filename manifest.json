{
  "manifest_version": 2,
  "name": "Image Clerk",
  "key":"",
  "description": "Search google's images, copy, download and paste fast",
  "version": "1.00",
  "author": "Gus Leyva",
  "background": {
  "scripts": ["js/background.js","js/glider.js"]
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"},
  "permissions":["identity","contextMenus","storage","notifications","tabs","activeTab","clipboardWrite"],  
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/*  ; object-src 'self'",
  "oauth2": {
    "client_id": "481015228871-0ngiisgdlp43usasbh7u5t9oflfmh0vl.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/chromewebstore.readonly"]
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "imageclerk.html"
  }     
}
